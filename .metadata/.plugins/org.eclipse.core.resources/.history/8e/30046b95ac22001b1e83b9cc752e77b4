package variaveis;

public class Tipos {

	public static void main(String[] args) {
	// tipo do dado  <nome da variavel/objeto>
	// String, int e double
	
		String nome = "Robert Rodrigues ";
		int idade = 50;
		double altura = 1.69;
		
		System.out.print(nome);
		System.out.print(idade);
		System.out.print(altura);
			
	
	}

}
